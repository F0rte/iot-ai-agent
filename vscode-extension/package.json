{
  "name": "agent-controller",
  "displayName": "Agent Controller",
  "description": "IoT AI Agent Controller for VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "main": "./out/extension.js",
  "activationEvents": ["onCommand:agentController.openPanel"],
  "contributes": {
    "commands": [
      {
        "command": "agentController.openPanel",
        "title": "AgentController: Open Panel"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^25.3.2",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.9.3"
  }
}
